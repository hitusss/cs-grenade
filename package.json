  "repository": {
    "type": "git",
    "url": "git+https://github.com/hitusss/cs-grenade.git"
  },
  "private": true,
  "sideEffects": false,
  "type": "module",
  "imports": {
    "#app/*": "./app/*",
    "#types/*": "./types/*",
    "#tests/*": "./tests/*"
  },
  "scripts": {
    "icon:add": "sly add lucide-icons",
    "build": "run-s build:*",
    "build:icons": "tsx ./other/build-icons.ts",
    "build:remix": "remix vite:build",
    "build:server": "tsx ./other/build-server.ts",
    "predev": "pnpm run build:icons --silent",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "dev": "node ./server/dev-server.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "setup": "pnpm run build && pnpm run db:deploy && pnpm run db:seed && pnpm run test:e2e:install",
    "start": "cross-env NODE_ENV=production node .",
    "start:mocks": "cross-env NODE_ENV=production MOCKS=true tsx .",
    "test:setup": "pnpm run build:icons",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e:setup": "pnpm run test:e2e:install && pnpm run db:deploy deploy && pnpm run build",
    "test:e2e:dev": "playwright test --ui",
    "test:e2e:run": "cross-env CI=true playwright test",
    "test:e2e:install": "pnpm exec playwright install --with-deps chromium",
    "typecheck": "tsc",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "postinstall": "pnpm run db:generate"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/playwright-report",
    "/server-build"
  ],
